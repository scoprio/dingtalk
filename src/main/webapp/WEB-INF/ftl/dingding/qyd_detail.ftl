		<script src="${basePath}/js/qifu/jquery-1.11.3.js" type="text/javascript" charset="utf-8"></script>
        <script src="${basePath}/js/qifu/common.js" type="text/javascript" charset="utf-8"></script>
		<script type="text/javascript">

			$(function() {

                $('.pl_box .pl_list').each(function(i,item){
                    var size = $(item).find('.star').attr('data-size');
                    var starNode = '';
                    for(var i= 0;i<size;i++){
                        starNode += '<li></li>'
                    }
                    $(item).find('.star').prepend(starNode)
                })

                $('.submit_btn').click(function() {

                    if('${qydInfo.isAdmin}' == 'true'){
                        location.href = "${basePath}/ulb/qf.shtml?corpid=${qydInfo.corpId}&appid="+localStorage.appId+"&cityCode=${qydInfo.cityCode}";
                    }else{
                        layer_tip("联系管理员开通");
                    }

                });

                $('.qyd_title li').click(function(){
                    $(this).find('a').addClass('change_color').parents().siblings().find('a').removeClass('change_color');
                    $('.qyd_content .content_right').eq($(this).index()).show().siblings().hide();
                })

				$('.refundment').click(function() {
						$('.refundment_bg').show()
				})
				$('.refundment_bg').click(function(){
					$(this).hide()
				})
                $('.refundment_box>i').click(function(){
                    $('.refundment_bg').hide()
                })
			})
		</script>
